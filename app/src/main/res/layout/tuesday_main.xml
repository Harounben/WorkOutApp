<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF323232">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#9C9C9C"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FF323232">

            <Button
                android:id="@+id/closeGripCurls2"
                android:layout_width="274dp"
                android:layout_height="48dp"
                android:layout_marginTop="16dp"
                android:onClick="completeButton"
                android:text="Pull downs 4 sets of 25"
                android:textColor="#000000"
                android:textSize="14sp"
                app:backgroundTint="#A5D6A7"
                app:cornerRadius="5mm"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.503"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/wideGripCurls3" />

            <Button
                android:id="@+id/closeGripCurls3"
                android:layout_width="274dp"
                android:layout_height="48dp"
                android:layout_marginTop="16dp"
                android:onClick="completeButton"
                android:text="Bike 1 Hour"
                android:textColor="#000000"
                android:textSize="14sp"
                app:backgroundTint="#A5D6A7"
                app:cornerRadius="5mm"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.491"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/wideGripCurls4" />

            <Button
                android:id="@+id/closeGripCurls4"
                android:layout_width="274dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="4dp"
                android:onClick="completeButton"
                android:text="Skip 1 hour"
                android:textColor="#000000"
                android:textSize="14sp"
                app:backgroundTint="#A5D6A7"
                app:cornerRadius="5mm"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.491"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/closeGripCurls3" />

            <Button
                android:id="@+id/hammerCurlsButton2"
                android:layout_width="274dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:onClick="completeButton"
                android:text="Dead Lifts 4 sets"
                android:textColor="#000000"
                android:textSize="14sp"
                app:backgroundTint="#A5D6A7"
                app:cornerRadius="5mm"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.503"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/button2" />

            <Button
                android:id="@+id/button2"
                android:layout_width="274dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:onClick="completeButton"
                android:text="Seated Rows 4 sets"
                android:textColor="#000000"
                android:textSize="14sp"
                app:backgroundTint="#A5D6A7"
                app:cornerRadius="5mm"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.503"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/closeGripCurls2" />

            <Button
                android:id="@+id/closeGripCurls"
                android:layout_width="274dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:onClick="completeButton"
                android:text="Skull crushers 4 sets"
                android:textColor="#000000"
                android:textSize="14sp"
                app:backgroundTint="#A5D6A7"
                app:cornerRadius="5mm"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/wideGripCurls" />

            <Button
                android:id="@+id/hammerCurlsButton"
                android:layout_width="274dp"
                android:layout_height="48dp"
                android:layout_marginTop="8dp"
                android:onClick="completeButton"
                android:text="Push UPS 4 sets of 25"
                android:textColor="#000000"
                android:textSize="14sp"
                app:backgroundTint="#A5D6A7"
                app:cornerRadius="5mm"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/closeGripCurls" />

            <Button
                android:id="@+id/wideGripCurls"
                android:layout_width="274dp"
                android:layout_height="48dp"
                android:layout_marginTop="16dp"
                android:onClick="completeButton"
                android:text="Pull Downs 4 sets"
                android:textColor="#000000"
                android:textSize="14sp"
                app:backgroundTint="#A5D6A7"
                app:cornerRadius="5mm"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/wideGripCurls2" />

            <Button
                android:id="@+id/wideGripCurls2"
                android:layout_width="250dp"
                android:layout_height="60dp"
                android:layout_marginTop="60dp"
                android:text="TRICEPS"
                android:textColor="#000000"
                android:textSize="30sp"
                app:backgroundTint="#FFFFFF"
                app:cornerRadius="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:strokeColor="#000000"
                app:strokeWidth="0px" />

            <Button
                android:id="@+id/wideGripCurls3"
                android:layout_width="250dp"
                android:layout_height="60dp"
                android:layout_marginTop="32dp"
                android:text="BACK"
                android:textColor="#000000"
                android:textSize="30sp"
                app:backgroundTint="#FFFFFF"
                app:cornerRadius="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/hammerCurlsButton"
                app:strokeColor="#FFFFFF"
                app:strokeWidth="0px" />

            <Button
                android:id="@+id/wideGripCurls4"
                android:layout_width="250dp"
                android:layout_height="60dp"
                android:layout_marginStart="4dp"
                android:layout_marginLeft="4dp"
                android:layout_marginTop="32dp"
                android:text="CARDIO"
                android:textColor="#000000"
                android:textSize="30sp"
                app:backgroundTint="#FFFFFF"
                app:cornerRadius="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/hammerCurlsButton2"
                app:strokeColor="#FFFFFF"
                app:strokeWidth="0px" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:background="#FFFFFF"
        android:text="                  "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:autoText="false"
        android:background="#000000"
        android:gravity="center"
        android:lineSpacingExtra="8sp"
        android:text="TUESDAY"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="78dp"
        android:layout_height="40dp"
        android:background="#000000"
        android:onClick="clickBackButton"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/abc_vector_test" />

</androidx.constraintlayout.widget.ConstraintLayout>
